{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "form-builder/input-select",
	"version": "0.1.0",
	"title": "Select",
	"category": "widgets",
	"icon": "smiley",
	"parent": ["form-builder/input-form"],
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"name": {
			"type": "string",
			"default": "spe-select-field"
		},
		"showLabel": {
			"type": "boolean",
			"default": true
		},
		"label": {
			"type": "string",
			"default": "Select"
		},
		"isRequired": {
			"type": "boolean",
			"default": true
		},
		"options": {
			"type": "array",
			"default": [
				{ "label": "Option 1", "value": "option_1" },
				{ "label": "Option 2", "value": "option_2" }
			]
		},
		"defaultValue": {
			"type": "string",
			"default": "option_2"
		},
		"useTaxonomy": {
			"type": "boolean",
			"default": false
		},
		"postType": {
			"type": "string",
			"default": ""
		},
		"taxonomy": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "result-spark-engine",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
